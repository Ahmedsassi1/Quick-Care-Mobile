{
  "expo": {
    "name": "quick",
    "slug": "quick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/urgence.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/urgence.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/urgence.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/urgence.png",
          "color": "#000000",
          "mode": "production"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/urgence.png"
    }
  }
}
